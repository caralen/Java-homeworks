{$= "text/html" @setMimeType $}
<html>
	<head>
		<title>Zbrajanje</title>
	</head>
	<body>
		<h1 align="center">Zbrajanje</h1>
	<table border="1" align="center">
	<tbody>
		<tr>
			<td>{$= "a" $}</td><td>{$= "a" "0" @tparamGet $}</td>
		</tr>
		<tr>
			<td>{$= "b" $}</td><td>{$= "b" "0" @tparamGet $}</td>
		</tr>
		<tr>
			<td>{$= "a+b" $}</td><td>{$= "zbroj" "0" @tparamGet $}</td>
		</tr>
	</tbody>
	</table>
	</body>
</html>